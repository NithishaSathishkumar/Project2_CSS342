./greedy_robot 2 -3 -3 5 5

echo “Test 14: (3, 1)”
./greedy_robot 4 0 0 0 3

echo “Test 15: (No path, 0)”
./greedy_robot 2 -4 -3 7 -1

echo “Test 16: (18, 2052)”
./greedy_robot 2 7 -4 -3 4

echo "Test 17: treasure and robot in same place"
./greedy_robot 1 1 1 1 1

