echo "Text x: (shortest distance, number of paths) \n"

echo "Test 1: not enough arguments"
./greedy_robot 1 2 3

echo "Test 2: too many arguments"
./greedy_robot 4 5 6 7 8 10 11

echo "Test 3: invaild max distance"
./greedy_robot -6 3 2 1 1

echo "Test 4: treasure and robot in same place"
./greedy_robot 5 3 3 3 3

echo "Test 5: (80, 9)”
./greedy_robot 5 -8 0 -11 -6

echo “Test 6: (14, 651)”
./greedy_robot 5 -1 0 -5 -10

echo “Test 7: (14, 2)”
./greedy_robot 1 2 8 9 1

echo “Test 8: (5, 7)”
./greedy_robot 2 1 2 3 5

echo “Test 9: (No path, 0)”
./greedy_robot 1 -1 2 2 9

